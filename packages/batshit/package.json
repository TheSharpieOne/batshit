{
  "name": "@yornaath/batshit",
  "version": "0.2.4",
  "description": "A batch manager that will deduplicate and batch requests for a certain data type made within a window.",
  "author": {
    "name": "JÃ¸rn Andre",
    "url": "https://github.com/yornaath/batshit"
  },
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./dist": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./dist/devtools": {
      "require": "./dist/devtools.cjs",
      "import": "./dist/devtools.mjs"
    }
  },
  "files": [
    "dist/*"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/yornaath"
  },
  "packageManager": "yarn@3.3.1",
  "scripts": {
    "test": "vitest run",
    "coverage": "vitest run --coverage",
    "build": "rm -rf ./dist && rollup -c && tsc"
  }
}
